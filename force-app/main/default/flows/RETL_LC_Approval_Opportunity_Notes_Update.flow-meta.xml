                </rightValue>
            </conditions>
            <connector>
                <targetReference>More_than_3_Rejected</targetReference>
            </connector>
            <label>Rejected</label>
        </rules>
    </decisions>
    <decisions>
        <name>More_than_3_Approved</name>
