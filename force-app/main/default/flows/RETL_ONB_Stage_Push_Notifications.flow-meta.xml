<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>65.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Check_for_Conceptual_Stage_and_tenant_email</name>
        <label>Check for Conceptual Stage and tenant email</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Check_Stage</name>
            <conditionLogic>(1 OR 2 OR 3 )AND 4 AND 5</conditionLogic>
            <conditions>
                <leftValueReference>$Record.RETL_Stage_Developer_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Conceptual_Design</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RETL_Stage_Developer_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Detailed_Architectural_Design</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RETL_Stage_Developer_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MEP_Design</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RETL_Service_RequestId__r.RETL_Tenant__r.Email</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Setup.RETL_Setting__c.RETL_Tenant_Push_Notifications__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Send_Push_Notifiaction</targetReference>
            </connector>
            <label>Check Stage</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>notForDocumentReview</name>
        <dataType>String</dataType>
        <expression>&quot;RDD Manager has reviewed your design. The Municipality NOC process is now in progress.&quot;</expression>
    </formulas>
    <formulas>
        <name>notTitleForDocumentReview</name>
        <dataType>String</dataType>
        <expression>&quot;Design Review Completed&quot;</expression>
    </formulas>
    <interviewLabel>RETL ONB {!$Flow.CurrentDateTime}</interviewLabel>
    <label>RETL ONB Stage Push Notifications</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Check_for_Conceptual_Stage_and_tenant_email</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>RETL_Document_Review_Status__c</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>RETL_Document_Review_Status__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <object>RETL_Stage__c</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
    <subflows>
        <name>Send_Push_Notifiaction</name>
        <label>Send Push Notifiaction</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <flowName>RETL_ServiceRequestCustomPushNotification</flowName>
        <inputAssignments>
            <name>CaseRecordId</name>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>NotificationBody</name>
            <value>
                <elementReference>notForDocumentReview</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>NotificationTitle</name>
            <value>
                <elementReference>notTitleForDocumentReview</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>UserEmail</name>
            <value>
                <elementReference>$Record.RETL_Service_RequestId__r.RETL_Tenant__r.Email</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>UserId</name>
            <value>
                <elementReference>$Record.RETL_Service_RequestId__r.RETL_Tenant__r.Email</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>UserName</name>
            <value>
                <elementReference>$Record.RETL_Service_RequestId__r.RETL_Tenant__r.Account.Name</elementReference>
            </value>
        </inputAssignments>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </subflows>
</Flow>
