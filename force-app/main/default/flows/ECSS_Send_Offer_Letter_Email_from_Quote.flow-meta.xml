    <formulas>
        <name>varF_isPaid</name>
        <dataType>String</dataType>
        <expression>IF(
  AND(
    NOT({!$Record.Opportunity.ECSS_TAF_Collection__c}),
    NOT({!$Record.Opportunity.ECSS_Security_Deposit_Collection__c})),
  &quot;Action Required: Please proceed with paying both the TAF and the Security Deposit.&quot;,

  IF(
    NOT({!$Record.Opportunity.ECSS_TAF_Collection__c}),
    &quot;Action Required: Kindly don&apos;t forget to pay the TAF before the deadline.&quot;,

    IF(
      NOT({!$Record.Opportunity.ECSS_Security_Deposit_Collection__c}),
      &quot;Action Required: Kindly don&apos;t forget to pay the Security Deposit.&quot;,
      &quot;&quot;
    )
  )
)</expression>
