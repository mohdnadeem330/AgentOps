        <name>CaseComments</name>
        <dataType>String</dataType>
        <expression>&quot;Previous: VIP Class=&quot; &amp; TEXT({!$Record.Previous_VIP_Class__c}) &amp;&quot;,Customer Type=&quot; &amp; TEXT({!$Record.Previous_Customer_Type__c}) &amp;&quot;, Customer Sub Type=&quot; &amp; TEXT({!$Record.Previous_Customer_Sub_Type__c}) &amp; &quot; | Upgraded to:: VIP Class=&quot; &amp; TEXT({!$Record.VIPClass__c}) &amp; &quot;, Customer Type=&quot; &amp; TEXT({!$Record.CustomerType__c}) &amp; &quot; , Customer Sub Type=&quot; &amp; TEXT({!$Record.CustomerSubType__c})</expression>
    </formulas>
    <formulas>
        <name>CaseSubject</name>
