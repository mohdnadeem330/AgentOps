    </formulas>
    <formulas>
        <description>check this customer already a VIP or not</description>
        <name>Checkexistingcustomer</name>
        <dataType>String</dataType>
        <expression>IF(
  AND(
    ISPICKVAL($Record.CustomerSubType__c, &quot;VIP TIER 1&quot;),
    OR(
      ISPICKVAL($Record.Proposed_New_Customer_Sub_type__c, &quot;VVIP TIER 2&quot;),
      ISPICKVAL($Record.Proposed_New_Customer_Sub_type__c, &quot;VVVIP TIER 3&quot;)
    )
  ),
  &quot;upgrade&quot;,
  IF(
    AND(
      ISPICKVAL($Record.CustomerSubType__c, &quot;VVIP TIER 2&quot;),
      ISPICKVAL($Record.Proposed_New_Customer_Sub_type__c, &quot;VVVIP TIER 3&quot;)
    ),
    &quot;upgrade&quot;,
    &quot;onboarding&quot;
  )
)</expression>
    </formulas>
