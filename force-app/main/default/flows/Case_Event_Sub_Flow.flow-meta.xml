    <formulas>
        <name>AccountCategory</name>
        <dataType>String</dataType>
        <expression>IF(
  {!CaseRecord.Account.RecordTypeName__c} = &quot;Person Account&quot;,
  &quot;1&quot;,
  IF(
    {!CaseRecord.Account.RecordTypeName__c} = &quot;OrganizationAccount&quot;,
    &quot;2&quot;,
    &quot;1&quot;
  )
)</expression>
    </formulas>
    <formulas>
        <name>AccountTitle</name>
        <dataType>String</dataType>
        <expression>
IF(
  {!CaseRecord.Account.RecordTypeName__c} = &quot;Person Account&quot;,
 IF(
    ISPICKVAL({!CaseRecord.Account.Salutation} ,  &quot;Mr.&quot;),
    &quot;0002&quot;,
    IF(
 ISPICKVAL({!CaseRecord.Account.Salutation} ,  &quot;Mrs.&quot;),
    &quot;0001&quot;,
    IF(
   ISPICKVAL({!CaseRecord.Contact.Gender__c} ,  &quot;Male&quot;),
    &quot;0002&quot;,
    &quot;0002&quot;
  )
  )
  )
,
  IF(
    {!CaseRecord.Account.RecordTypeName__c} = &quot;OrganizationAccount&quot;,
    &quot;0003&quot;,
    IF(
    ISPICKVAL({!CaseRecord.Contact.Gender__c} ,  &quot;Male&quot;),
    &quot;0002&quot;,
    &quot;0002&quot;
  )
  )
)</expression>
    </formulas>
    <formulas>
