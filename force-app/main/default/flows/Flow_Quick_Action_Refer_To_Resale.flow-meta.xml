    <decisions>
        <name>Check_If_we_need_to_Assigned_to_Resale_Queue</name>
        <label>Check If we need to Assigned to Resale Queue</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>WarningScreen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not assign</defaultConnectorLabel>
        <rules>
            <name>Assign_to_Resale</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Lead_Record.Owner:Group.Id</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>Fetch_Resale_Queue.Id</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Lead_Project</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Lead_Record_to_Resale_Queue</targetReference>
            </connector>
            <label>Assign to Resale</label>
        </rules>
        <rules>
            <name>Project_not_found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Lead_Project</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>WarningScreen2</targetReference>
            </connector>
            <label>Project not found</label>
        </rules>
    </decisions>
