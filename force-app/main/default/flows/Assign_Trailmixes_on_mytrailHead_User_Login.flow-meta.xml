    <description>This flow creates Trailmix Assignment records for the user created for the qualified Trailmix Assignment Critierias</description>
    <environments>Default</environments>
    <formulas>
        <name>TACCalculatedDueDate</name>
        <dataType>Date</dataType>
        <expression>IF(!ISBLANK({!TACDueDate}),{!TACDueDate},IF({!TACMaximumDays}&gt;0,Today()+{!TACMaximumDays},null))</expression>
    </formulas>
    <formulas>
        <name>TACCalculatedDueDateTime</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE({!TACCalculatedDueDate}) - (4/24)</expression>
    </formulas>
    <formulas>
        <name>userScope</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!Fetch_User_Record.UserType}) == &apos;Standard&apos;,&apos;Internal&apos;,&apos;External&apos;)</expression>
    </formulas>
