    <decisions>
        <name>Start_Navigation_Filter_Conditions</name>
        <label>Start Navigation Filter Conditions</label>
        <locationX>446</locationX>
        <locationY>242</locationY>
        <defaultConnector>
            <targetReference>Enroute_checking</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>If_AMC_Appointment</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_ServiceAppointmentRecord.Record_Type_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>AMC_Assessment</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>AMCWarning</targetReference>
            </connector>
            <label>If AMC Appointment</label>
        </rules>
        <rules>
            <name>Is_partner_SA</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_ServiceAppointmentRecord.Partner_Service_Appointments__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_ServiceAppointmentRecord.Record_Type_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Resolution</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>DLP_Information_Page</targetReference>
            </connector>
            <label>Is partner SA</label>
        </rules>
        <rules>
            <name>Enroute_status</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>var_ServiceAppointmentRecord.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Scheduled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_ServiceAppointmentRecord.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Re-scheduled</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_SA</targetReference>
            </connector>
            <label>Enroute status</label>
        </rules>
    </decisions>
