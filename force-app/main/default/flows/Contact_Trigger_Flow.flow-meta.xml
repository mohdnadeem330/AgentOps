        <expression>ISCHANGED({!$Record.NextBirthday__c})</expression>
    </formulas>
    <formulas>
        <name>NextBirthdateCalculation</name>
        <dataType>Date</dataType>
        <expression>IF(
TODAY()&gt; DATE(YEAR(TODAY()),MONTH({!$Record.Birthdate}),DAY({!$Record.Birthdate})),DATE(YEAR(TODAY())+1,MONTH({!$Record.Birthdate}),DAY({!$Record.Birthdate})),DATE(YEAR(TODAY()),MONTH({!$Record.Birthdate}),DAY({!$Record.Birthdate})))</expression>
    </formulas>
    <formulas>
