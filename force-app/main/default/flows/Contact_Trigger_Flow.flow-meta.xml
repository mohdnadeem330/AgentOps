    <formulas>
        <name>BirthdayPlus1Year</name>
        <dataType>Date</dataType>
        <expression>IF(
  AND(
    MONTH( {!$Record.NextBirthday__c}) = 2,
    DAY( {!$Record.NextBirthday__c}) = 29,
    NOT( 
      OR( 
        MOD( YEAR( {!$Record.NextBirthday__c}) + 1, 400 ) = 0, 
        AND( 
          MOD( YEAR( {!$Record.NextBirthday__c}) + 1, 4 ) = 0,
          MOD( YEAR( {!$Record.NextBirthday__c}) + 1, 100 ) != 0
        )
      )
    ) 
  ),
  DATE( YEAR( {!$Record.NextBirthday__c}) + 1, 3, 1),
  DATE( YEAR( {!$Record.NextBirthday__c}) + 1, MONTH( {!$Record.NextBirthday__c}), DAY( {!$Record.NextBirthday__c}) )
)</expression>
    </formulas>
    <formulas>
        <name>fml_WebName</name>
        <dataType>String</dataType>
        <expression>{!$Record.FirstName} +&apos; &apos;+{!$Record.LastName}</expression>
    </formulas>
    <formulas>
        <name>IsChanged</name>
