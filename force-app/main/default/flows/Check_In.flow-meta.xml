        <rules>
            <name>If_AMC_Appointment</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_ServicceAppointmentRecord.Record_Type_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>AMC_Assessment</stringValue>
