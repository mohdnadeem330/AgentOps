    <decisions>
        <name>IfStatus_scheduled</name>
        <label>status check</label>
        <locationX>842</locationX>
        <locationY>458</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>If_status_scheduled</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Work In Progress</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Partially_fixed</targetReference>
            </connector>
            <label>If status==Dispatched</label>
        </rules>
        <rules>
            <name>status_scheduled</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Scheduled</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>screen_for_partially</targetReference>
            </connector>
            <label>status == scheduled</label>
        </rules>
        <rules>
            <name>if_status_completed_or_closed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WOstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WOstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_1_of_screen_for_partially</targetReference>
            </connector>
            <label>if status=completed or closed</label>
        </rules>
        <rules>
            <name>if_status_partially_completed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Partially Completed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Partially_completed_screen</targetReference>
            </connector>
            <label>if status=partially completed</label>
        </rules>
    </decisions>
