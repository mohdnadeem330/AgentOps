        <actionType>outboundMessage</actionType>
        <connector>
            <targetReference>Update_Lead</targetReference>
        </connector>
        <flowTransactionModel>CurrentTransaction</flowTransactionModel>
        <nameSegment>Task.LSQ_Initital_Enquiries_UAT</nameSegment>
        <offset>0</offset>
