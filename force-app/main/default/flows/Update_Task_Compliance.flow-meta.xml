<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>64.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Update_Pre_Compliance_to_In_Progress</name>
        <label>Update Pre-Compliance to In-Progress</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>Get_Sales_Order.Pre_Compliance_Status__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Under Review</stringValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_Sales_Order_In_Progress</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Update_Task_Status_to_Retired</name>
        <label>Update Task Status to Retired</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>Get_Existing_Screening_Task.Status</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Retired</stringValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_Task</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Is_Task_Completed</name>
        <label>Is Task Completed</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Is_Completed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordTypeId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Get_Screening_Task_Record_Type.Id</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Sales_Order</targetReference>
            </connector>
            <label>Is Completed</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Task_Completed_by_Prescreening_or_SM</name>
        <label>Is Task Completed by Prescreening or SM</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default</defaultConnectorLabel>
        <rules>
            <name>IsTaskCompletedbyPrescreening</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.LastModifiedBy.Profile.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sales Admin</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Pre_Screening_Date_Check</targetReference>
            </connector>
            <label>Is Task Completed by Prescreening</label>
        </rules>
        <rules>
            <name>Is_Task_Completed_by_Sales_Manager</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.LastModifiedBy.Profile.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sales Manager</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Complince_Pre_Screening_Queue</targetReference>
            </connector>
            <label>Is Task Completed by Sales Manager</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Task_Exists</name>
        <label>Is Task Exists</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Create_Task_for_Pre_Screening_Team</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Task_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Existing_Screening_Task</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Task_Status_to_Retired</targetReference>
            </connector>
            <label>Task Exists</label>
        </rules>
    </decisions>
    <decisions>
        <name>Pre_Screening_Date_Check</name>
        <label>Pre Screening Date Check</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Update_Sales_Order_Status</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Is_Pre_screening_Date_Blank</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Sales_Order.Pre_Screening_Completed_Date__c</leftValueReference>
                <operator>EqualTo</operator>
            </conditions>
            <connector>
                <targetReference>Update_Sales_Order</targetReference>
            </connector>
            <label>Is Pre-screening Date Blank</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>CompletedDate</name>
        <dataType>Date</dataType>
        <expression>DATEVALUE({!$Record.CompletedDateTime})</expression>
    </formulas>
    <formulas>
        <name>DueDate</name>
        <dataType>Date</dataType>
        <expression>TODAY() + VALUE({!$Label.Task_Due_in_Days})</expression>
    </formulas>
    <formulas>
        <name>TaskSubject</name>
        <dataType>String</dataType>
        <expression>&quot;Pre Screening Request &quot; &amp; {!Get_Sales_Order.Name}</expression>
    </formulas>
    <formulas>
        <name>TaskUrl</name>
        <dataType>String</dataType>
        <expression>HYPERLINK(&apos;https://yourDomain.lightning.force.com/&apos;&amp;{!Create_Task_for_Pre_Screening_Team}, &apos;Task Name&apos;)</expression>
    </formulas>
    <interviewLabel>Update Task Compliance {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Update Task Compliance</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
