<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>64.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Assign_Lead_Type</name>
        <label>Assign Lead Type</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>LeadType</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Direct</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>LeadSource</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Aldar</stringValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Sodic_Event</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Copy_1_of_Assign_Lead_Type</name>
        <label>Assign Lead Type</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>LeadType</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Indirect</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>LeadSource</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Broker</stringValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Copy_1_of_Sodic_Event</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Check_Lead_Type</name>
        <label>Check Lead Type</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Direct</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.BrokerAgencyAccount__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Lead_Type</targetReference>
            </connector>
            <label>Direct</label>
        </rules>
        <rules>
            <name>Indirect</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.BrokerAgencyAccount__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_1_of_Assign_Lead_Type</targetReference>
            </connector>
            <label>Indirect</label>
        </rules>
    </decisions>
    <decisions>
        <description>Check for new Lead then insert platform event
otherwise failed lead then insert platform event</description>
        <name>New_Lead_Or_Old_Lead</name>
        <label>New Lead Or Old Lead</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>new_lead_or_old_lead_condition</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>DelimatedProjectList</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>WrappedProjectName</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_Lead_Type</targetReference>
            </connector>
            <label>new lead or old lead condition</label>
        </rules>
    </decisions>
    <description>SODIC Event Creation Flow</description>
    <environments>Default</environments>
    <formulas>
        <name>DelimatedProjectList</name>
        <dataType>String</dataType>
        <expression>&quot;,&quot; &amp; {!projectList} &amp; &quot;,&quot;</expression>
    </formulas>
    <formulas>
        <name>MobileNumberCustom</name>
        <dataType>String</dataType>
        <expression>&quot;+&quot; &amp; {!$Record.Account.PersonMobilePhone}</expression>
    </formulas>
    <formulas>
        <name>Product</name>
        <dataType>String</dataType>
        <expression>IF(
  TEXT({!$Record.UnitType__c}) = &quot;Apartment&quot;, &quot;Chalet Apartment&quot;,
  IF(
    TEXT({!$Record.UnitType__c}) = &quot;Villa&quot;, &quot;Medium Villa&quot;,
    IF(
      TEXT({!$Record.UnitType__c}) = &quot;Townhouse&quot;, &quot;Townhouse Middle&quot;,
      &quot;Medium Villa&quot;
    )
  )
)</expression>
    </formulas>
    <formulas>
        <name>WrappedProjectName</name>
        <dataType>String</dataType>
        <expression>&quot;,&quot; &amp; TEXT( {!$Record.Project__c}) &amp; &quot;,&quot;</expression>
    </formulas>
    <interviewLabel>SODIC Event Creation Flow {!$Flow.CurrentDateTime}</interviewLabel>
    <label>SODIC Event Creation Flow</label>
    <processMetadataValues>
        <name>BuilderType</name>
