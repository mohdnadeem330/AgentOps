    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>New_Lead_Or_Old_Lead</targetReference>
        </connector>
        <filterFormula>OR(
    ISNEW(),
    AND(
        ISPICKVAL({!$Record.SODIC_Sync_Status__c}, &quot;In Progress&quot;),
        PRIORVALUE({!$Record.SODIC_Sync_Status__c}) = &quot;Failed&quot;
    )
)</filterFormula>
        <object>Opportunity</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
