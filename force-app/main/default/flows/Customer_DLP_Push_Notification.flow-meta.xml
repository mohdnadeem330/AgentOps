    <formulas>
        <name>AccountName</name>
        <dataType>String</dataType>
        <expression>IF(
  $Record.Account.IsPersonAccount,
  $Record.Account.FirstName &amp; &quot; &quot; &amp; $Record.Account.LastName,
  $Record.Account.Name
)</expression>
    </formulas>
    <formulas>
        <name>AppointmentBookingDate</name>
        <dataType>String</dataType>
        <expression>CASE(
  MONTH(DATEVALUE({!GetServiceAppointment.SchedStartTime})),
  1, &quot;January&quot;,
  2, &quot;February&quot;,
  3, &quot;March&quot;,
  4, &quot;April&quot;,
  5, &quot;May&quot;,
  6, &quot;June&quot;,
  7, &quot;July&quot;,
  8, &quot;August&quot;,
  9, &quot;September&quot;,
  10, &quot;October&quot;,
  11, &quot;November&quot;,
  12, &quot;December&quot;,
  &quot;Unknown&quot;
)
&amp; &quot; &quot; &amp;
TEXT(DAY(DATEVALUE({!GetServiceAppointment.SchedStartTime}))) &amp; &quot;, &quot; &amp;
TEXT(YEAR(DATEVALUE({!GetServiceAppointment.SchedStartTime})))</expression>
    </formulas>
    <formulas>
        <name>BookingTime</name>
        <dataType>String</dataType>
        <expression>IF(
   MOD(HOUR(TIMEVALUE({!GetServiceAppointment.SchedStartTime})), 12) = 0,
   &quot;12&quot;,
   TEXT(MOD(HOUR(TIMEVALUE({!GetServiceAppointment.SchedStartTime})), 12))
) &amp; &quot;:&quot; &amp;
LPAD(TEXT(MINUTE(TIMEVALUE({!GetServiceAppointment.SchedStartTime}))), 2, &quot;0&quot;) &amp;
IF(HOUR(TIMEVALUE({!GetServiceAppointment.SchedStartTime})) &lt; 12, &quot; AM&quot;, &quot; PM&quot;)</expression>
    </formulas>
