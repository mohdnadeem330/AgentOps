            <conditions>
                <leftValueReference>rec_SA.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Scheduled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>rec_SA.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Re-scheduled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>rec_SA.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>En Route</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Login_Warning</targetReference>
            </connector>
            <label>If Status = Assigned</label>
        </rules>
        <rules>
            <name>If_Work_In_Progress</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>rec_SA.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Work In Progress</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>rec_SA.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Assessment Completed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Close_screen</targetReference>
            </connector>
            <label>If Work In Progress</label>
        </rules>
    </decisions>
