    <decisions>
        <name>check_status</name>
        <label>check status</label>
        <locationX>864</locationX>
        <locationY>350</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>status_dispatched_or_InProgress</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>En Route</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Work In Progress</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>screen_for_mark_as_complete</targetReference>
            </connector>
            <label>status==dispatched</label>
        </rules>
        <rules>
            <name>if_status_check_status</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WOstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WOstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Task_Completed</targetReference>
            </connector>
            <label>if status==check status</label>
        </rules>
        <rules>
            <name>ifStatus_partiallyfixed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Partially Completed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Partially_Complted</targetReference>
            </connector>
            <label>ifStatus==partiallyfixed</label>
        </rules>
        <rules>
            <name>IfStatus_nonfixable</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Non Fixable</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Non_fixable</targetReference>
            </connector>
            <label>IfStatus==nonfixable</label>
        </rules>
        <rules>
            <name>If_Status_Assigned_or_Rescheduled</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Scheduled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Re-scheduled</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Error_Assigned_or_Rescheduled</targetReference>
            </connector>
            <label>If Status == Assigned or Rescheduled</label>
        </rules>
    </decisions>
