    <decisions>
        <name>Filtering_using_status</name>
        <label>Filtering using status</label>
        <locationX>578</locationX>
        <locationY>242</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>scheduled_Dispached</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Work In Progress</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Scheduled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Assessment Completed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>DLP_comments</targetReference>
            </connector>
            <label>scheduled - Dispached</label>
        </rules>
        <rules>
            <name>if_status_scheduled</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>En Route</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Not_in_DLP_scope</targetReference>
            </connector>
            <label>if status=scheduled</label>
        </rules>
        <rules>
            <name>if_status_completed_or_closed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WOstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WOstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_1_of_Not_in_DLP_scope</targetReference>
            </connector>
            <label>if status=completed or closed</label>
        </rules>
        <rules>
            <name>if_Status_Partially_Completed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SAstatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Partially Completed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>PartiallyFixedScreen</targetReference>
            </connector>
            <label>if Status == Partially Completed</label>
        </rules>
    </decisions>
