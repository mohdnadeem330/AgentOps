        <locationX>446</locationX>
        <locationY>242</locationY>
        <defaultConnector>
            <targetReference>QuickFixValidation</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>ifStatus_completed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Var_ServiceAppoinemnt.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Var_ServiceAppoinemnt.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Appontment_Completed</targetReference>
            </connector>
            <label>ifStatus=completed or closed</label>
        </rules>
        <rules>
            <name>if_Status_Scheduled</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Var_ServiceAppoinemnt.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Scheduled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Var_ServiceAppoinemnt.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Screen_for_check_In</targetReference>
            </connector>
            <label>if Status == Scheduled</label>
        </rules>
        <rules>
            <name>For_Quick_Fix_Condition</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Var_ServiceAppoinemnt.Quick_Fix__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ClossureComments</targetReference>
            </connector>
            <label>For Quick Fix Condition</label>
        </rules>
    </decisions>
