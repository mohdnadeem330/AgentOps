        <rules>
            <name>Screening_Result_is_Proceed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.tr_wc1__Risk_Resolution__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>No Risk Identified - Proceed</stringValue>
                </rightValue>
            </conditions>
