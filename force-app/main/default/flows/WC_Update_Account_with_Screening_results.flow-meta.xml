                    <stringValue>Risk Identified - Proceed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Account_AML_Fields_as_Proceed</targetReference>
            </connector>
            <label>Screening Result is Proceed</label>
        </rules>
        <rules>
            <name>Screening_Result_NOT_to_Proceed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.tr_wc1__Risk_Resolution__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Risk Identified - Do not Proceed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Account_AML_fields_as_do_not_proceed</targetReference>
            </connector>
            <label>Screening Result NOT to Proceed</label>
        </rules>
