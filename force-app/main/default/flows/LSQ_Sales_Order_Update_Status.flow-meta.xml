    <formulas>
        <name>fmlaDiscountPercent</name>
        <dataType>Number</dataType>
        <expression>({!curDiscount} / {!Get_Sales_Order.LSQ_Asking_Price__c}) * 100</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fmlaNetAmount</name>
        <dataType>Currency</dataType>
        <expression>{!Get_Sales_Order.LSQ_Asking_Price__c} - {!curDiscount}</expression>
        <scale>2</scale>
    </formulas>
